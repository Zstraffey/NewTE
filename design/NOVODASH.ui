                        <string notr="true">/* ======================= */
/* Tabela geral */
QTableWidget {
    background-color: #2b2b2b;
    color: #f0f0f0;
    border: none;
    gridline-color: #444444;
    font-size: 14px;
    alternate-background-color: #242424;
    selection-background-color: #3a86ff;
    selection-color: #ffffff;
}

/* ======================= */
/* Cabeçalho */
QHeaderView::section {
    background-color: #1f1f1f;
    color: #e0e0e0;
    border: none;
    border-bottom: 1px solid #555555;
    padding: 8px;
    font-weight: bold;
    font-size: 13px;
    text-align: center;
}

/* ======================= */
/* Itens */
QTableWidget::item {
    border-bottom: 1px solid #444444;
    padding: 8px;
}
QTableWidget::item:hover {
    background-color: #333333;
    color: #ffffff;
}

/* ======================= */
/* Última linha */
QTableCornerButton::section {
    background-color: #2b2b2b;
    border: none;
}

/* ======================= */
/* Botões */
QPushButton {
    background-color: #3a86ff;
    color: #ffffff;
    border: none;
    border-radius: 6px;
    padding: 6px 12px;
    font-weight: bold;
}
QPushButton:hover {
    background-color: #265d9d;
}
QPushButton:pressed {
    background-color: #1b3b63;
}

/* ======================= */
/* Campo de busca */
QLineEdit {
    background-color: #1f1f1f;
    border: 1px solid #555555;
    border-radius: 6px;
    color: #ffffff;
    padding: 6px 10px;
}
QLineEdit:focus {
    border: 1px solid #3a86ff;
}

/* ======================= */
/* ComboBox (Filtro) */
QComboBox {
    background-color: #1f1f1f;
    color: #ffffff;
    border: 1px solid #555555;
    border-radius: 6px;
    padding: 5px 10px;
}
QComboBox::drop-down {
    border: none;
}
QComboBox QAbstractItemView {
    background-color: #2b2b2b;
    color: #ffffff;
    selection-background-color: #3a86ff;
    selection-color: #ffffff;
}
